{
  "name": "wepublish",
  "private": true,
  "engines": {
    "node": "^22.20.0"
  },
  "sideEffects": false,
  "scripts": {
    "migrate": "prisma migrate deploy && prisma db seed",
    "serve": "nx run-many  --parallel 10 -t serve --output-style stream",
    "dev": "run-s start:docker watch",
    "clean": "run-p --print-label clean:**",
    "clean:media": "docker volume rm wepublish_storage_data",
    "lint": "nx run-many --target=lint",
    "build": "nx run-many --targets=build",
    "i18n": "node_modules/i18next-parser/bin/cli.js --config i18next-parser.config.js",
    "test": "nx run-many --target=test --parallel=false --runInBand",
    "test-u": "nx run-many --target=test --parallel=false --runInBand --u",
    "test-backend": "nx run-many --target=test --parallel=10 --runInBand --exclude='*website*'",
    "test-website": "nx run-many --target=test --parallel=3 --runInBand --projects=*website*",
    "watch:editor": "nx serve editor",
    "watch:api-example": "nx serve api-example",
    "watch:website-example": "nx serve website-example",
    "watch:media": "nx serve media",
    "start:docker": "docker-compose up --detach database storage",
    "watch": "npm run migrate && nx run-many --target=serve --output-style=stream --projects=api-example,editor,media,website-example",
    "generate-api": "graphql-codegen -r dotenv/config --config codegen.yml",
    "try": "docker-compose up database migration api editor media storage",
    "prettier": "prettier --write \"{apps,libs}/**/*.{js,ts,tsx,json}\"",
    "prepare": "prisma generate",
    "postinstall": "node build/postInstall.js",
    "deploy:production": "node deployment/deploy_production.js",
    "deploy:production:tags": "bash deployment/clean_deploy_tags.sh",
    "setup:production:local": "bash scripts/setup-local-production-database.sh"
  },
  "nx": {
    "includedScripts": [
      "generate-api"
    ]
  },
