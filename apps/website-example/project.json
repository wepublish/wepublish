{
  "name": "website-example",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/website-example/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx nx browser website-example"
          },
          {
            "command": "npx nx server website-example"
          }
        ]
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:ssr-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "browserTarget": "website-example:browser:development",
        "serverTarget": "website-example:serve-server:development",
        "port": 4200,
        "browserTargetOptions": {
          "watch": true
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "browserTarget": "website-example:build:production",
          "serverTarget": "website-example:serve-server:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/website-example/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/website-example/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "browser": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/website-example/browser",
        "index": "apps/website-example/src/index.html",
        "baseHref": "/",
        "main": "apps/website-example/src/main.tsx",
        "polyfills": "apps/website-example/src/polyfills.ts",
        "tsConfig": "apps/website-example/tsconfig.app.json",
        "assets": ["apps/website-example/src/assets"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/website-example/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/website-example/src/environments/environment.ts",
              "with": "apps/website-example/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "server": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "main": "apps/website-example/server.ts",
        "outputPath": "dist/apps/website-example",
        "tsConfig": "apps/website-example/tsconfig.server.json",
        "compiler": "babel",
        "externalDependencies": "all",
        "generatePackageJson": true,
        "outputHashing": "none",
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "sourceMap": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/website-example/src/environments/environment.ts",
              "with": "apps/website-example/src/environments/environment.prod.ts"
            }
          ],
          "sourceMap": false
        }
      }
    },
    "serve-browser": {
      "executor": "@nrwl/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "website-example:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "website-example:build:development"
        },
        "production": {
          "buildTarget": "website-example:build:production",
          "hmr": false
        }
      }
    },
    "serve-server": {
      "executor": "@nrwl/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "website-example:server:development",
        "buildTargetOptions": {
          "watch": true
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "buildTarget": "website-example:server:production"
        }
      }
    }
  },
  "tags": []
}
