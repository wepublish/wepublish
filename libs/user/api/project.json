{
  "name": "user-api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/user/api/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/user/api",
        "tsConfig": "libs/user/api/tsconfig.lib.json",
        "packageJson": "libs/user/api/package.json",
        "main": "libs/user/api/src/index.ts",
        "assets": ["libs/user/api/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/user/api/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/user/api/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
