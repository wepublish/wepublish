{
  "name": "@wepublish/website-example",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/server/index.js",
  "license": "UNLICENSED",
  "engines": {
    "node": "^12.0.0",
    "yarn": "^1.17.0"
  },
  "scripts": {
    "build": "run-p build:client build:server",
    "build:client": "webpack --mode production --devtool source-map",
    "build:server": "tsc -b ./src/shared ./src/server",
    "build:storybook": "build-storybook -c ./src/shared/.storybook -o ./.storybookbuild",
    "watch": "npm-run-all --parallel watch:client watch:server",
    "watch:client": "webpack-dev-server --mode development --hot --inline",
    "watch:server": "run-p watch:server:*",
    "watch:server:start": "nodemon --inspect=0.0.0.0:9229 ./dist/server/index.js",
    "watch:server:build": "tsc -b ./src/server ./src/shared --watch",
    "watch:storybook": "start-storybook --ci -h 0.0.0.0 -p 3003 -s ./ -c ./src/shared/.storybook",
    "clean": "shx rm -rf ./dist ./assets",
    "start": "node ./dist/server/index.js"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@karma.run/react": "^0.1.2",
    "@karma.run/utility": "^0.1.0",
    "@karma.run/webpack": "^0.0.3",
    "cross-fetch": "^3.0.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fela": "^10.6.1",
    "fela-dom": "^10.6.1",
    "fela-plugin-fallback-value": "^10.6.1",
    "fela-plugin-prefixer": "^10.6.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.9.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.9.0",
    "react-helmet-async": "^1.0.2",
    "slate": "^0.52.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@hot-loader/react-dom": "^16.8.6",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.10",
    "@storybook/react": "^5.1.9",
    "@types/express": "^4.17.1",
    "@types/node": "^12.0.12",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-helmet-async": "^1.0.0",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__react": "^4.0.2",
    "@types/webpack": "^4.4.34",
    "@types/webpack-dev-server": "^2.9.4",
    "babel-loader": "^8.0.6",
    "core-js": "^3.2.1",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "react-hot-loader": "^4.12.3",
    "regenerator-runtime": "^0.13.3",
    "shx": "^0.3.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.8.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  }
}
