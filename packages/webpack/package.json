{
  "name": "@wepublish/webpack",
  "version": "0.0.0",
  "main": "./lib/index.js",
  "license": "MIT",
  "scripts": {
    "build": "run-s copy-license sync-package-info build:*",
    "build:typescript": "tsc -b ./src",
    "watch": "run-s copy-license sync-package-info watch:*",
    "watch:typescript": "tsc -b ./src --watch",
    "copy-license": "shx cp ../../LICENSE ./",
    "clean": "shx rm -rf ./LICENSE ./lib",
    "prepare": "run-s clean copy:license build sync-package-info",
    "sync-package-info": "node ./../../scripts/syncPackageInfo.js ./src/version.ts"
  },
  "devDependencies": {
    "@types/node": "^12.0.12",
    "@types/webpack": "^4.4.34",
    "typescript": "^3.5.2",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.2"
  },
  "peerDependencies": {
    "webpack": "^4.35.3"
  }
}
