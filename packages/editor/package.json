{
  "name": "@wepublish/editor",
  "version": "0.7.3",
  "bin": {
    "wepublish-editor": "./dist/server/index.js"
  },
  "main": "./dist/server/index.js",
  "license": "MIT",
  "scripts": {
    "setup": "shx cp .env.example .env",
    "build": "run-p build:*",
    "build:client": "webpack --mode production",
    "build:server": "tsc -b ./src/shared ./src/server",
    "watch": "npm-run-all copy-license sync-package-info --parallel watch:*",
    "watch:client": "webpack-dev-server --mode development --hot --inline",
    "watch:server": "run-p watch:server:*",
    "watch:server:start": "nodemon -r dotenv/config --inspect=0.0.0.0:9230 ./dist/server/index.js",
    "watch:server:build": "tsc -b ./src/shared ./src/server --watch",
    "watch:generate": "yarn generate --watch \"./src/**/*.graphql\"",
    "crlf": "crlf --set=LF dist/**/*",
    "clean": "shx rm -rf ./dist ./assets",
    "start": "node ./dist/server/index.js",
    "copy-license": "shx cp ../../LICENSE ./",
    "sync-package-info": "node ./../../scripts/syncPackageInfo.js ./src/shared/version.ts",
    "prepare": "run-s clean copy-license sync-package-info build crlf",
    "generate": "graphql-codegen -r dotenv/config --config codegen.yml"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@karma.run/icons": "^1.1.0",
    "@karma.run/react": "^0.1.3",
    "@karma.run/ui": "0.3.20",
    "@karma.run/webpack": "^0.0.3",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-upload-client": "^12.1.0",
    "apollo-utilities": "^1.3.2",
    "array-move": "^2.2.1",
    "core-js": "^3.4.8",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "nanoid": "^2.1.7",
    "pretty-bytes": "5.3.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-sortable-hoc": "^1.11.0",
    "regenerator-runtime": "^0.13.3",
    "slate": "^0.58.1",
    "slate-history": "^0.58.1",
    "slate-hyperscript": "^0.58.1",
    "slate-react": "^0.58.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@graphql-codegen/add": "^1.13.5",
    "@graphql-codegen/cli": "^1.13.5",
    "@graphql-codegen/introspection": "^1.13.5",
    "@graphql-codegen/typescript": "^1.13.5",
    "@graphql-codegen/typescript-operations": "^1.13.5",
    "@graphql-codegen/typescript-react-apollo": "^1.13.5",
    "@hot-loader/react-dom": "^16.11.0",
    "@types/apollo-upload-client": "^8.1.3",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.16",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-server": "^3.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "crlf": "^1.1.1",
    "dotenv": "^8.2.0",
    "fork-ts-checker-webpack-plugin": "^4.1.4",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.2",
    "ts-node": "^8.5.4",
    "typescript": "^3.8.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
