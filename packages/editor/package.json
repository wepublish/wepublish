{
  "name": "@wepublish/editor",
  "version": "0.1.0",
  "bin": {
    "wepublish-editor": "./dist/server/index.js"
  },
  "main": "./dist/server/index.js",
  "license": "MIT",
  "scripts": {
    "build": "run-p build:*",
    "build:client": "webpack --mode production --devtool source-map",
    "build:server": "tsc -b ./src/shared ./src/server",
    "watch": "npm-run-all copy-license sync-package-info --parallel watch:*",
    "watch:client": "webpack-dev-server --mode development --hot --inline",
    "watch:server": "run-p watch:server:*",
    "watch:server:start": "nodemon --inspect=0.0.0.0:9230 ./dist/server/index.js",
    "watch:server:build": "tsc -b ./src/shared ./src/server --watch",
    "clean": "shx rm -rf ./dist ./assets",
    "start": "node ./dist/server/index.js",
    "copy-license": "shx cp ../../LICENSE ./",
    "sync-package-info": "node ./../../scripts/syncPackageInfo.js ./src/shared/version.ts",
    "prepare": "run-s clean copy-license sync-package-info build"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@karma.run/icons": "^1.0.0",
    "@karma.run/react": "0.1.0",
    "@karma.run/ui": "0.1.1",
    "@karma.run/webpack": "^0.0.3",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-upload-client": "^11.0.0",
    "apollo-utilities": "^1.3.2",
    "core-js": "^3.1.4",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "immutable": "^4.0.0-rc.12",
    "pretty-bytes": "^5.3.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.12.3",
    "regenerator-runtime": "^0.13.3",
    "slate": "^0.47.9",
    "slugify": "^1.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/apollo-upload-client": "^8.1.3",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.1",
    "@types/node": "^12.0.12",
    "@types/react": "^16.9.7",
    "@types/react-dom": "^16.9.2",
    "@types/webpack": "^4.4.34",
    "@types/webpack-dev-server": "^2.9.4",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.3.0",
    "typescript": "^3.7.2",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  }
}
