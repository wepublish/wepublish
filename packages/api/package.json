{
  "name": "@wepublish/api",
  "version": "0.0.9",
  "module": "./lib/esm/client/",
  "license": "MIT",
  "scripts": {
    "build": "run-s copy-license sync-package-info build:*",
    "build:typescript": "tsc -b ./src/client/tsconfig.json ./src/client/tsconfig.esm.json ./src/server",
    "watch": "npm-run-all copy-license sync-package-info --parallel watch:*",
    "watch:typescript": "tsc -b ./src/client/tsconfig.json ./src/client/tsconfig.esm.json ./src/server --watch",
    "copy-license": "shx cp ../../LICENSE ./",
    "clean": "shx rm -rf ./LICENSE ./lib",
    "prepare": "run-s clean copy-license build sync-package-info",
    "sync-package-info": "node ./../../scripts/syncPackageInfo.js ./src/client/version.ts"
  },
  "dependencies": {
    "@karma.run/graphql": "^0.0.0",
    "jsonwebtoken": "^8.5.1",
    "ms": "^2.1.2",
    "cross-fetch": "^3.0.4",
    "apollo-client": "^2.6.4",
    "apollo-server": "^2.9.7",
    "graphql": "^14.5.8",
    "graphql-iso-date": "^3.6.1",
    "graphql-upload": "8.1.0",
    "nanoid": "^2.1.2"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "@types/graphql": "^14.5.0",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/graphql-upload": "^8.0.3",
    "@types/nanoid": "^2.0.0",
    "@types/node": "^12.6.8",
    "@types/jsonwebtoken": "7.2.7",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.2",
    "typescript": "^3.6.4"
  }
}
